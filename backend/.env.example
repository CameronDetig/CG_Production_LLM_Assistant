# Environment variables for Lambda function
# Copy this to .env and fill in your actual values

# PostgreSQL Database Configuration
DB_HOST=your-rds-endpoint.xxxxx.us-east-1.rds.amazonaws.com
DB_NAME=your_database_name
DB_USER=your_username
DB_PASSWORD=your_password
DB_PORT=5432

# AWS Configuration
# Note: AWS_REGION is automatically set by Lambda (uses deployment region)
# Only set AWS_REGION locally for testing

# Bedrock Model Configuration
BEDROCK_MODEL_ID=us.meta.llama3-2-11b-instruct-v1:0
BEDROCK_LARGE_MODEL_ID=us.meta.llama3-1-70b-instruct-v1:0

